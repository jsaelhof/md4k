{
  "name": "md4k-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn generate && vite --port 4000",
    "build": "yarn generate && yarn tsc && vite build",
    "preview": "yarn generate && vite preview",
    "test": "yarn generate && yarn tsc && vitest",
    "test-ui": "yarn generate && vitest --ui",
    "coverage": "yarn generate && vitest run --coverage",
    "generate": "yarn workspace md4k-constants build && yarn ts-typegen && yarn i18n-typegen",
    "ts-typegen": "graphql-codegen --config codegen.ts",
    "i18n-typegen": "find ./src -path '*/i18n/en/*.json' | node ./src/i18next/generate.js",
    "typecheck": "yarn tsc --noEmit",
    "typecheck:tests": "npx tsc --noEmit",
    "lint": "npx eslint \"./src/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "prettier": "npx prettier --write .",
    "tsc": "npx tsc -p ./tsconfig.build.json"
  },
  "dependencies": {
    "@apollo/client": "^3.11.4",
    "@auth0/auth0-react": "2.2.4",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "5.16.7",
    "@mui/material": "5.16.7",
    "@mui/styles": "5.16.7",
    "apollo3-cache-persist": "^0.15.0",
    "date-fns": "^3.6.0",
    "graphql": "16.9.0",
    "i18next": "^23.12.2",
    "i18next-icu": "^2.3.0",
    "intl-messageformat": "^10.5.14",
    "lodash": "^4.17.21",
    "md4k-constants": "*",
    "mdi-material-ui": "^7.9.1",
    "omit-deep-lodash": "^1.1.7",
    "react": "^18.3.1",
    "react-day-picker": "9.0.8",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.2",
    "react-i18next": "^15.0.1",
    "react-router-dom": "6.26.0",
    "react-spring": "^9.7.4",
    "react-youtube": "10.1.0",
    "rooks": "^7.14.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.3.3",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/css-mediaquery": "^0.1.4",
    "@types/lodash": "^4.17.7",
    "@types/material-ui": "^0.21.17",
    "@types/omit-deep-lodash": "^1.1.3",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^2.1.1",
    "css-mediaquery": "^0.1.2",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "glob": "^11.0.0",
    "i18next-resources-for-ts": "^1.5.0",
    "jsdom": "^25.0.0",
    "vite": "^5.4.5",
    "vitest": "^2.1.1"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*"
  }
}
