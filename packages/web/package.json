{
  "name": "md4k-web",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn run generate && vite --port 4000",
    "build": "yarn run generate && npx tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint \"./src/**/*.{js,jsx,ts,tsx}\" --max-warnings=0",
    "prettier": "npx prettier --write .",
    "test": "vitest",
    "test-ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "generate": "yarn run ts-typegen && yarn run i18n-typegen",
    "ts-typegen": "graphql-codegen --config codegen.ts",
    "i18n-typegen": "find ./src -path '*/i18n/en/*.json' | node ./src/i18next/generate.js",
    "typecheck": "npx tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@auth0/auth0-react": "^1.10.2",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "5.15.2",
    "@mui/material": "5.15.2",
    "@mui/styles": "5.15.2",
    "apollo3-cache-persist": "^0.14.1",
    "date-fns": "^2.29.2",
    "graphql": "16.6.0",
    "i18next": "^23.5.1",
    "i18next-icu": "^2.3.0",
    "intl-messageformat": "^10.5.2",
    "lodash": "^4.17.21",
    "md4k-constants": "*",
    "mdi-material-ui": "^7.6.0",
    "omit-deep-lodash": "^1.1.7",
    "react": "^18.2.0",
    "react-day-picker": "8.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "react-i18next": "^13.2.2",
    "react-router-dom": "6.10.0",
    "react-spring": "^9.5.2",
    "react-youtube": "10.1.0",
    "rooks": "^5.11.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "6.2.0",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/css-mediaquery": "^0.1.4",
    "@types/lodash": "^4.14.202",
    "@types/material-ui": "^0.21.12",
    "@types/omit-deep-lodash": "^1.1.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^2.0.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.22.1",
    "css-mediaquery": "^0.1.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.3.4",
    "i18next-resources-for-ts": "^1.4.0",
    "jsdom": "^20.0.0",
    "vite": "^3.0.0",
    "vitest": "^0.34.3"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*"
  }
}
